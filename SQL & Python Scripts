   "source": [
    "#What are the top 3 most purchased products within each category? \n",
    "pd.read_sql_query(\"\"\"\n",
    " \n",
    "  with cte1 as(select category, item_purchased, \n",
    "        count(*) total_purchase \n",
    "     from cleaned_data  \n",
    "     group by category, item_purchased),\n",
    "\n",
    "  cte_rank as(\n",
    "  select category, item_purchased, total_purchase, \n",
    "  row_number()over(partition by category order by total_purchase desc) rnk \n",
    "  from cte1\n",
    "  \n",
    "  ) \n",
    "\n",
    "  select category, item_purchased, total_purchase \n",
    "      from cte_rank \n",
    "      where rnk <=3\"\"\",conn)\n",
    "     \n",
    "     \n",
    "     \n",
    "     \n",
    "     \n",
    "     \n",
    "\n",
    "\n"
   ]
